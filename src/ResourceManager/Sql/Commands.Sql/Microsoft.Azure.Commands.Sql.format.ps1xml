              </ListItem>
			        <ListItem>
                <Label>AllowReadOnlyFailoverToPrimary</Label>
                <PropertyName>ReadOnlyFailoverPolicy</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
  </ViewDefinitions>
</Configuration>
